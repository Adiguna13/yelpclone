<% layout('layouts/app') %>
<h1>Add Place</h1>
<form action="/places" method="POST">
  <div>
    <label for="title">title</label>
    <input type="text" name="place[title]" id="title" />
  </div>
  <div>
    <label for="location">location</label>
    <input type="text" name="place[location]" id="location" />
  </div>
  <div>
    <label for="description">description</label>
    <input type="text" name="place[description]" id="description" />
  </div>
  <div>
    <label for="price">price</label>
    <input type="text" name="place[price]" id="price" />
  </div>
  <button type="submit">Save</button>
</form>
